{
  "name": "react-ts-start-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "host": "vite --host",
    "stylelint": "npx stylelint \"./**/*.{css,scss,sass}\" --config \"./stylelint.config.cjs\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier:fix": "prettier --config ./prettier.config.cjs --write ./src/**/*.{css,scss,ts,tsx} ",
    "prettier:check": "prettier --config ./prettier.config.cjs --check ./src/**/*.{css,scss,ts,tsx}",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint",
    "./src/*.{css,scss,sass}": "npm run stylelint",
    "./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier -w -u"
  },

  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-vitest-globals": "1.2.0",
    "husky": "^8.0.3",
    "postcss": "8.4.19",
    "postcss-extend": "1.0.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.0.0",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "6.0.0",
    "postcss-preset-env": "7.8.3",
    "postcss-reporter": "7.0.5",
    "prettier": "2.8.0",
    "sass": "^1.69.5",
    "stylelint": "14.15.0",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-recommended-scss": "8.0.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-stylelint": "3.2.0"
  }
}
